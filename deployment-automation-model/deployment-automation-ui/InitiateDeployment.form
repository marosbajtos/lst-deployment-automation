<?xml version="1.0" encoding="UTF-8"?>
<com.whitestein.lsps.modeler.form:FormDefinition
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:com.whitestein.lsps.modeler.form="http:///com/whitestein/lsps/modeler/form.ecore"
    xmi:id="_lSMBcK5VEeSsJdpZZuml9w"
    name="InitiateDeployment"
    newUI="true">
  <formItem
      xsi:type="com.whitestein.lsps.modeler.form:NfdPanel"
      xmi:id="_pLSZ8K5VEeSsJdpZZuml9w">
    <child
        xsi:type="com.whitestein.lsps.modeler.form:NfdLayout"
        xmi:id="_A0YLIK5WEeSsJdpZZuml9w">
      <components
          xsi:type="com.whitestein.lsps.modeler.form:NfdFormLayout"
          xmi:id="_A9VS0K5WEeSsJdpZZuml9w">
        <components
            xsi:type="com.whitestein.lsps.modeler.form:NfdTextArea"
            xmi:id="_A9VS0a5WEeSsJdpZZuml9w">
          <binding><![CDATA[&description]]></binding>
          <label><![CDATA["Description"]]></label>
        </components>
        <components
            xsi:type="com.whitestein.lsps.modeler.form:NfdComboBox"
            xmi:id="_A9VS0q5WEeSsJdpZZuml9w">
          <binding><![CDATA[&target]]></binding>
          <label><![CDATA["Target"]]></label>
          <options>getTargetOptions()</options>
        </components>
      </components>
      <components
          xsi:type="com.whitestein.lsps.modeler.form:NfdButton"
          xmi:id="_By7eEK5WEeSsJdpZZuml9w">
        <listeners
            xmi:id="_Ms0_0K5WEeSsJdpZZuml9w"
            listenerType="ACTION_LISTENER">
          <submitAction
              xmi:id="_59mrIK5WEeSsJdpZZuml9w"/>
          <handle><![CDATA[createModelInstance(
	true, 
	getFirst(findModels("deployment-automation-process", "*", true)), 
	[
		"description" -> description,
		"target" -> target
	]
)]]></handle>
        </listeners>
        <text><![CDATA["Initiate"]]></text>
      </components>
    </child>
    <title><![CDATA["Initiate deployment"]]></title>
  </formItem>
  <variables
      xmi:id="_ujt3MK5VEeSsJdpZZuml9w"
      name="description"
      variableType="String"/>
  <variables
      xmi:id="_wmBtMK5VEeSsJdpZZuml9w"
      name="target"
      variableType="String"/>
</com.whitestein.lsps.modeler.form:FormDefinition>
